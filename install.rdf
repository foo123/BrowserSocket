<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>browsersocket@hiit.fi</em:id>
        <em:version>1.3.4</em:version>
        <em:type>2</em:type>
        <em:creator>konker</em:creator>
        <em:name>BrowserSocket</em:name>
        <em:description>
            An extension implementing the BrowserSocket framework.
            BrowserSockets make it possible to build untrusted javascript applications 
            that listen for WebSocket connections from other browsers.
        </em:description>
        <em:homepageURL>http://www.hiit.fi/</em:homepageURL>
        <!--
        <em:iconURL>chrome://icon.png</em:iconURL>
        <em:unpack>true</em:unpack>
        -->

        <!-- Target application, i.e. FF -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>3.6</em:minVersion>
                <em:maxVersion>4.0b12</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
</RDF>
